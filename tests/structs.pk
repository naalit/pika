val Vec3 = sig
    val x : I32
    val y : I32
    val z : I32

val up : Vec3 = struct
    val y = 1
    val z = 0
    val x = 0

# Making sure unification ignores order
val AlsoVec3 = sig
    val x : I32
    val z : I32
    val y : I32
val q : AlsoVec3 = up

# A test of dependent records i.e. sigma types
val Any = sig
    val T : Type
    val x : T
val three : Any = struct
    val T = I32
    val x = 3
# Making sure unification works with dependent records
val AlsoAny = sig
    val T : Type
    val x : T
val also_three : AlsoAny = three

fun print_vec3 (v : Vec3) with IO = do
    puts "{\n\t"
    print (v.x)
    puts "\t"
    print (v.y)
    puts "\t"
    print (v.z)
    puts "}\n"

fun main () with IO = print_vec3 up