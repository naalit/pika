# Testing error messages associated with currying
fun plus x y : I32 = x + y

let too_few : I32 = plus 1
let too_many : I32 = plus 1 2 3
let just_right : I32 = plus 1 2
# This shouldn't give us an argument error, just a could not match error
let none : I32 = plus

fun inc x : I32 = x + 1
let argument_singular = inc 2 3
